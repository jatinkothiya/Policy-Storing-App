    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton2ActionPerformed
String a,b,c;
a=bn.getText();
b=bc.getSelectedItem().toString();
c=bp.getText();
Integer myInt=(Integer)yu.getValue();
String spinner=myInt.toString();


        if(a.isEmpty())
            JOptionPane.showMessageDialog(this,"Please Enter Bike's Details");
        else
            if(b.equals("----------"))
            JOptionPane.showMessageDialog(this,"Please Enter Bike's Details"); 
else
                 if(c.isEmpty())
            JOptionPane.showMessageDialog(this,"Please Enter Bike's Details");
        
                     
        double rs;
        rs = Double.parseDouble(c)*0.1+Double.parseDouble(c)*0.02+Double.parseDouble(c)*myInt/100;
        if(c1.isSelected())
            rs+=412;
           if(c2.isSelected())
            rs+=825;
              if(c3.isSelected())
            rs+=320;
                 if(c4.isSelected())
            rs+=399;
                 jButton2.setText(""+rs);

       // TODO add your handling code here:
    }//GEN-LAST:event_jButton2ActionPerformed

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton3ActionPerformed

String a,b,c;
a=bn.getText();
b=bc.getSelectedItem().toString();
c=bp.getText();
Integer myInt=(Integer)yu.getValue();
String spinner=myInt.toString();


        if(a.isEmpty())
            JOptionPane.showMessageDialog(this,"Please Enter Bike's Details");
        else
            if(b.equals("----------"))
            JOptionPane.showMessageDialog(this,"Please Enter Bike's Details"); 
else
                 if(c.isEmpty())
            JOptionPane.showMessageDialog(this,"Please Enter Bike's Details");
        
                     
        double rs;
        rs = Double.parseDouble(c)*0.1+Double.parseDouble(c)*0.04+Double.parseDouble(c)*myInt/100;
        if(a1.isSelected())
            rs+=500;
           if(a2.isSelected())
            rs+=1010;
              if(a3.isSelected())
            rs+=480;
                 if(a4.isSelected())
            rs+=426;
                 jButton3.setText(""+rs);
     // TODO add your handling code here:
    }//GEN-LAST:event_jButton3ActionPerformed

    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton4ActionPerformed
String a,b,c;
a=bn.getText();
b=bc.getSelectedItem().toString();
c=bp.getText();
Integer myInt=(Integer)yu.getValue();
String spinner=myInt.toString();
        if(a.isEmpty())
            JOptionPane.showMessageDialog(this,"Please Enter Bike's Details");
        else
           if(b.equals("----------"))
            JOptionPane.showMessageDialog(this,"Please Enter Bike's Details"); 
else
                 if(c.isEmpty())
            JOptionPane.showMessageDialog(this,"Please Enter Bike's Details");
        
                     
        double rs=Double.parseDouble(c)*0.1-Double.parseDouble(c)*0.02+Double.parseDouble(c)*myInt/100;
        if(b1.isSelected())
            rs+=452;
           if(b2.isSelected())
            rs+=910;
              if(b3.isSelected())
            rs+=423;
                 if(b4.isSelected())
            rs+=575;
                 jButton4.setText(""+rs);


        // TODO add your handling code here:
    }//GEN-LAST:event_jButton4ActionPerformed

    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton5ActionPerformed
