            JOptionPane.showMessageDialog(this,"Please Enter Bike's Details");
        else
           if(b.equals("----------"))
            JOptionPane.showMessageDialog(this,"Please Enter Bike's Details"); 
else
                 if(c.isEmpty())
            JOptionPane.showMessageDialog(this,"Please Enter Bike's Details");
        
                     
        double rs=Double.parseDouble(c)*0.1+Double.parseDouble(c)*0.005+Double.parseDouble(c)*myInt/100;
        if(d1.isSelected())
            rs+=364;
           if(d2.isSelected())
            rs+=724;
              if(d3.isSelected())
