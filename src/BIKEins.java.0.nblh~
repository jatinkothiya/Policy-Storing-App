    private void jButton8ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton8ActionPerformed
String bn;
bn=JOptionPane.showInputDialog(this,"<html><font face=rockwell size=7>Enter Bike's Number",null,3);
        try{
     Class.forName("java.sql.Driver");
    Connection conn =DriverManager.getConnection("jdbc:mysql://localhost/jk","root","mysql");
    Statement st=conn.createStatement();
String qry="select * from bike;";
    ResultSet rs=st.executeQuery(qry);
    while(rs.next()){
        String ei=rs.getString("bike_no");
        String in=rs.getString("yr_use");
        String p=rs.getString("price");
        String bc=rs.getString("bike_com");
        String ins=rs.getString("INS_COMP");
        String co=rs.getString("Totalcover_Rs");
        String pr=rs.getString("premium");
        if(bn.equals(ei))
            JOptionPane.showMessageDialog(this,"<html><font face=rockwell size=6>Bike No. :- "+ei+"<br>Years Used :- "+in+"<br>Price :- "+p+"<br>Bike's Company :- "+bc+"<br>Insurance Company :- "+ins+"<br>Cover Added Amount :- "+co+"<br>Premium per Annum :- "+pr,null,2);
}
    
    if(!rs.next())
        JOptionPane.showMessageDialog(this,"<html><font face=rockwell size=6>Bike's Number not Found <br> Please Add to Cart first",null,0);
}
catch(Exception ss){

}
    // TODO add your handling code here:
    }//GEN-LAST:event_jButton8ActionPerformed

    private void jButton10ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton10ActionPerformed
String a,b,c,j;

a=bn.getText();
b=bc.getSelectedItem().toString();
c=bp.getText();



Integer myInt=(Integer)yu.getValue();
String spinner=myInt.toString();


        String bn;
        float prem,cov;
         
        String cab[]=new String[4];
        cab[0]="HDFC";
        cab[1]="Exide Life";
        cab[2]="LIC";
        cab[3]="Kotak Life";
    if(cab.equals(cab[0])){
   prem=Float.parseFloat(jButton2.getText()); 
   float rs1;
       rs1=  (float) (Float.parseFloat(c)*0.1+Float.parseFloat(c)*0.02+Float.parseFloat(c)*myInt/100);
   cov=prem-rs1;
    }
else        if(cab.equals(cab[1])){
   prem=Float.parseFloat(jButton3.getText());
                
        double rs2;
        rs2 = Double.parseDouble(c)*0.1+Double.parseDouble(c)*0.04+Double.parseDouble(c)*myInt/100;
cov=        (float) (prem-rs2);
   
        }
    else    if(cab.equals(cab[2])){
   prem=Float.parseFloat(jButton4.getText());
    double rs=Double.parseDouble(c)*0.1-Double.parseDouble(c)*0.02+Double.parseDouble(c)*myInt/100;
cov=        (float)(prem-rs);
        }
    else    if(cab.equals(cab[3])){
            if(d1.isSelected())
    cov+=364;

if(d2.isSelected())
    cov+=724;

if(d3.isSelected())
    cov+=285;

if(d4.isSelected())
    cov+=680;

            
           prem=jButton5.getText();
        }     
        
bn=JOptionPane.showInputDialog(this,"<html><font face=rockwell size=7>Enter Bike's Number",null,3);
Object cp=JOptionPane.showInputDialog(null,null, "Select Company :- ",JOptionPane.QUESTION_MESSAGE,null,cab,"HDFC");
        try{
     Class.forName("java.sql.Driver");
    Connection conn =DriverManager.getConnection("jdbc:mysql://localhost/jk","root","mysql");
    Statement st=conn.createStatement();
String qry="Update bike set bike_no='"+a+"',yr_use="+myInt+",price="+c+",bike_com='"+b+"',INS_COMP='"+cab+"',Totalcover_Rs="+cov+",premium="+prem+" where bike_no='"+bn+"';";
           st.executeUpdate(qry);
    
    
        }
        catch(Exception tt){
        }
    // TODO add your handling code here:
    }//GEN-LAST:event_jButton10ActionPerformed

