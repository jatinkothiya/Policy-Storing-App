    private void but1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_but1ActionPerformed
String a,b,c,d;

a=ci.getText();
b=ea.getText();
c=mem1.getText();
d=con.getText();
if(a.isEmpty() || b.isEmpty() || c.isEmpty() || d.isEmpty()){
    JOptionPane.showMessageDialog(this,"<html><font face=rockwell size=15>Please Enter Details",null,3);}
Float cov=0f;
if(a1.isSelected())
cov+=470;
if(a2.isSelected())
cov+=2140;
if(a3.isSelected())
cov+=0;


        
Float m,pre,p1 = null,p2 = null,p3 = null,p4 = null,p5 = null,a1 = null,a2 = null,a3 = null,a4 = null,a5 = null;
m=Float.parseFloat(b);


    

String cb = (String)jComboBox1.getSelectedItem();
switch (cb){
    case "Self":
    a1=Float.parseFloat(age1.getText());
p1=(m*a1)/(m/1000);
p2=0f;
p3=0f;
p4=0f;
p5=0f;
        break;

    case "Spouse":
         a1=Float.parseFloat(age1.getText());
p1=(m*a1)/(m/400);
a2=Float.parseFloat(age2.getText());
p2=(m*a2)/(m/400);
p3=0f;
p4=0f;
p5=0f;
        break;
        

    case "Child-1":
         a1=Float.parseFloat(age1.getText());
p1=(m*a1)/(m/400);
a2=Float.parseFloat(age2.getText());
p2=(m*a2)/(m/400);
a3=Float.parseFloat(age3.getText());
p3=(m*a3)/(m/400);
p4=0f;
p5=0f;
break;

    case "Child-2":
        a1=Float.parseFloat(age1.getText());
p1=(m*a1)/(m/400);
a2=Float.parseFloat(age2.getText());
p2=(m*a2)/(m/400);
a3=Float.parseFloat(age3.getText());
p3=(m*a3)/(m/400);
a4=Float.parseFloat(age4.getText());
p4=(m*a4)/(m/400);
p5=0f;
break;

    case "Child-3":
         a1=Float.parseFloat(age1.getText());
p1=(m*a1)/(m/400);
a2=Float.parseFloat(age2.getText());
p2=(m*a2)/(m/400);
a3=Float.parseFloat(age3.getText());
p3=(m*a3)/(m/400);
a4=Float.parseFloat(age4.getText());
p4=(m*a4)/(m/400);
a5=Float.parseFloat(age5.getText());
p5=(m*a5)/(m/400);
        break;
}


pre=p1+p2+p3+p4+p5+cov;
but1.setText(""+pre);

// TODO add your handling code here:
    }//GEN-LAST:event_but1ActionPerformed

