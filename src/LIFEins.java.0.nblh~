    private void jComboBox1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jComboBox1ActionPerformed
String tr=(String)jComboBox1.getSelectedItem();
switch(tr){
    case "1":
        mem2.setVisible(false);
mem3.setVisible(false);
mem4.setVisible(false);
mem5.setVisible(false);
age2.setVisible(false);
age3.setVisible(false);
age4.setVisible(false);
age5.setVisible(false);
nm2.setVisible(false);
nm3.setVisible(false);
nm4.setVisible(false);
nm5.setVisible(false);
        break;
    case "2":
        mem2.setVisible(true);
mem3.setVisible(false);
mem4.setVisible(false);
mem5.setVisible(false);
age2.setVisible(true);
age3.setVisible(false);
age4.setVisible(false);
age5.setVisible(false);
nm2.setVisible(true);
nm3.setVisible(false);
nm4.setVisible(false);
nm5.setVisible(false);
        

        break;
        case "3":
            mem2.setVisible(true);
mem3.setVisible(true);
mem4.setVisible(false);
mem5.setVisible(false);
age2.setVisible(true);
age3.setVisible(true);
age4.setVisible(false);
age5.setVisible(false);
nm2.setVisible(true);
nm3.setVisible(true);
nm4.setVisible(false);
nm5.setVisible(false);
         
    
        break;
            case "4":
                mem2.setVisible(true);
mem3.setVisible(true);
mem4.setVisible(true);
mem5.setVisible(false);
age2.setVisible(true);
age3.setVisible(true);
age4.setVisible(true);
age5.setVisible(false);
nm2.setVisible(true);
nm3.setVisible(true);
nm4.setVisible(true);
nm5.setVisible(false);
                
  
        break;
                case "5":
                 
                    mem2.setVisible(true);
        age2.setVisible(true);
   nm2.setVisible(true); 
        mem3.setVisible(true);
        age3.setVisible(true);
   nm3.setVisible(true);
        mem4.setVisible(true);
        age4.setVisible(true);
   nm4.setVisible(true); 
        mem5.setVisible(true);
        age5.setVisible(true);
   nm5.setVisible(true);    
 
        break;
        
} 
        // TODO add your handling code here:
    }//GEN-LAST:event_jComboBox1ActionPerformed

