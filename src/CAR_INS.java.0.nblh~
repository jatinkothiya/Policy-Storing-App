    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton2ActionPerformed
String a,b,c;
Integer myInt=(Integer)yu.getValue();
String spinner=myInt.toString();
a=cn.getText();
b=cc.getSelectedItem().toString();
c=cp.getText();
        if(a.isEmpty())
            JOptionPane.showMessageDialog(this,"Please Enter Car's Details");
        else

                 if(c.isEmpty())
            JOptionPane.showMessageDialog(this,"Please Enter Car's Details");
    else
                     if(b.equals("----------"))
                        JOptionPane.showMessageDialog(this,"Please Enter Car's Details");
        
        double rs=Double.parseDouble(c)*0.02-Double.parseDouble(c)*0.003+Double.parseDouble(c)*myInt/100;
        if(a1.isSelected())
            rs+=710;
           if(a2.isSelected())
            rs+=1210;
              if(a3.isSelected())
            rs+=2260;
                 if(a4.isSelected())
            rs+=540;
                 jButton2.setText("Rs."+rs);        // TODO add your handling code here:
    }//GEN-LAST:event_jButton2ActionPerformed

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton3ActionPerformed
String a,b,c;
a=cn.getText();
Integer myInt=(Integer)yu.getValue();
String spinner=myInt.toString();
b=cc.getSelectedItem().toString();
c=cp.getText();
        if(a.isEmpty())
            JOptionPane.showMessageDialog(this,"Please Enter Car's Details");
        else

                 if(c.isEmpty())
            JOptionPane.showMessageDialog(this,"Please Enter Car's Details");
   else
                                          if(b.equals("----------"))
                        JOptionPane.showMessageDialog(this,"Please Enter Car's Details");
                     
        double rs=Double.parseDouble(c)*0.02+Double.parseDouble(c)*0.001+Double.parseDouble(c)*myInt/100;
        if(b1.isSelected())
            rs+=814;
           if(b2.isSelected())
            rs+=915;
              if(b3.isSelected())
            rs+=2021;
                 if(b4.isSelected())
            rs+=625;
                 jButton3.setText("Rs."+rs);        // TODO add your handling code here:
    }//GEN-LAST:event_jButton3ActionPerformed

    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton4ActionPerformed
String a,b,c;
a=cn.getText();
Integer myInt=(Integer)yu.getValue();
String spinner=myInt.toString();
b=cc.getSelectedItem().toString();
c=cp.getText();
        if(a.isEmpty())
            JOptionPane.showMessageDialog(this,"Please Enter Car's Details");
        else
          if(c.isEmpty())
            JOptionPane.showMessageDialog(this,"Please Enter Car's Details");
   else
                                          if(b.equals("----------"))
                        JOptionPane.showMessageDialog(this,"Please Enter Car's Details");      
                     
        double rs=Double.parseDouble(c)*0.02-Double.parseDouble(c)*0.001+Double.parseDouble(c)*myInt/100;
        if(c1.isSelected())
            rs+=584;
           if(c2.isSelected())
            rs+=1045;
              if(c3.isSelected())
            rs+=2140;
                 if(c4.isSelected())
            rs+=209;
                 jButton4.setText("Rs."+rs);          // TODO add your handling code here:
    }//GEN-LAST:event_jButton4ActionPerformed

    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton5ActionPerformed
String a,b,c;
a=cn.getText();
Integer myInt=(Integer)yu.getValue();
String spinner=myInt.toString();
b=cc.getSelectedItem().toString();
c=cp.getText();
        if(a.isEmpty())
            JOptionPane.showMessageDialog(this,"Please Enter Car's Details");
        else

                 if(c.isEmpty())
            JOptionPane.showMessageDialog(this,"Please Enter Car's Details");
   else
                                          if(b.equals("----------"))
                        JOptionPane.showMessageDialog(this,"Please Enter Car's Details");      
                     
        double rs=Double.parseDouble(c)*0.02+Double.parseDouble(c)*0.003+Double.parseDouble(c)*myInt/100;
        if(d1.isSelected())
            rs+=699;
           if(d2.isSelected())
            rs+=650;
              if(d3.isSelected())
            rs+=3610;
                 if(d4.isSelected())
            rs+=775;
                 jButton5.setText("Rs."+rs);        // TODO add your handling code here:
    }//GEN-LAST:event_jButton5ActionPerformed

    private void jButton9ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton9ActionPerformed
String a,b,c,j;
int rs=0; 
a=cn.getText();
b=cc.getSelectedItem().toString();
c=cp.getText();
j="HDFC";
 if(a1.isSelected())
            rs+=710;
           if(a2.isSelected())
            rs+=1210;
              if(a3.isSelected())
            rs+=2260;
                 if(a4.isSelected())
            rs+=540;



Integer myInt=(Integer)yu.getValue();
String spinner=myInt.toString();


        if(a.isEmpty() || b.equals("----------") || c.isEmpty())
            JOptionPane.showMessageDialog(this,"Please Enter Bike's Details");
   
         try{
            Class.forName("java.sql.Driver");
    Connection conn =DriverManager.getConnection("jdbc:mysql://localhost/jk","root","mysql");
    Statement st=conn.createStatement();
String qry="select * from car;";
    ResultSet rs=st.executeQuery(qry);
while(rs.next()){
        String ei=rs.getString("bike_no");
        if(a.equals(ei))
            JOptionPane.showMessageDialog(this, "<html><font face=rockwell size=6>Already Added on this Number<br>If you want to Mofify Details Please Click on MODIFY DETAILS",null,3);
}
            if(!rs.next()){
            String query="Insert into bike values('"+a+"',"+myInt+","+c+",'"+b+"','"+j+"',"+rs+","+jButton2.getText()+");";
            st.executeUpdate(query);
            JOptionPane.showMessageDialog(this, "<html><font face=rockwell size=15>Added to Cart Successfully",null,2);
            }
                 
  }
       catch(Exception e){
       
       } 
        // TODO add your handling code here:
    }//GEN-LAST:event_jButton9ActionPerformed

